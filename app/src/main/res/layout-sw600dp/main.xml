<?xml version="1.0" encoding="utf-8"?>
    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="40dp" >

        <Button
            android:id="@+id/button10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            android:layout_marginBottom="10dp"
            android:layout_marginLeft="40dp"
            android:layout_marginRight="40dp"
            android:layout_marginTop="10dp"
            android:paddingBottom="15dp"
            android:paddingLeft="30dp"
            android:paddingRight="30dp"
            android:paddingTop="15dp"
            android:text="@string/connect"
            android:textSize="15dp" 
            android:onClick="onCLickCardConnect"/>

        <Button
            android:id="@+id/button11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/button10"
            android:layout_alignParentRight="true"
            android:layout_below="@id/button10"
            android:layout_marginBottom="10dp"
            android:layout_marginRight="40dp"
            android:layout_marginTop="10dp"
            android:paddingBottom="15dp"
            android:paddingLeft="18dp"
            android:paddingRight="17dp"
            android:paddingTop="15dp"
            android:text="@string/Reconnect"
            android:textSize="15dp" 
            android:onClick="onCLickCardReConnect" />

        <Button
            android:id="@+id/button12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_below="@id/button11"
            android:layout_centerVertical="false"
            android:layout_marginBottom="10dp"
            android:layout_marginRight="40dp"
            android:layout_marginTop="10dp"
            android:layout_toRightOf="@+id/button15"
            android:padding="15dp"
            android:text="@string/Disconnect"
            android:textSize="15dp" 
            android:onClick="onCLickCardDisConnect" />

        <Button
            android:id="@+id/button13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/button14"
            android:layout_marginLeft="40dp"
            android:layout_marginRight="40dp"
            android:layout_marginTop="10dp"
            android:layout_toLeftOf="@+id/button11"
            android:paddingBottom="15dp"
            android:paddingLeft="25dp"
            android:paddingRight="25dp"
            android:paddingTop="15dp"
            android:text="@string/req"
            android:textSize="15dp" 
            android:onClick="onCLickSelectReq" />

        <Button
            android:id="@+id/button15"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/button12"
            android:layout_alignBottom="@+id/button12"
            android:layout_marginBottom="10dp"
            android:layout_marginLeft="40dp"
            android:layout_marginRight="40dp"
            android:layout_marginTop="10dp"
            android:layout_toLeftOf="@+id/button11"
            android:paddingBottom="15dp"
            android:paddingLeft="31dp"
            android:paddingRight="31dp"
            android:paddingTop="15dp"
            android:text="@string/RunScenario"
            android:textSize="15dp" 
            android:onClick="onCLickRunScenario" />

        <Button
            android:id="@+id/button14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/button11"
            android:layout_alignBottom="@+id/button11"
            android:layout_alignLeft="@+id/button15"
            android:layout_marginBottom="10dp"
            android:layout_marginRight="40dp"
            android:layout_marginTop="10dp"
            android:layout_toLeftOf="@+id/button11"
            android:paddingBottom="15dp"
            android:paddingLeft="21dp"
            android:paddingRight="21dp"
            android:paddingTop="15dp"
            android:text="@string/NextCommand"
            android:textSize="15dp" 
            android:onClick="onCLickNextCommand" />

        <Button
            android:id="@+id/button08"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/button12"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/button12"
            android:layout_marginBottom="10dp"
            android:layout_marginRight="40dp"
            android:layout_marginTop="10dp"
            android:padding="15dp"
            android:text="@string/scardcontrol"
            android:textSize="15dp" 
            android:onClick="onCLickSCardControl" />

        <Button
            android:id="@+id/button16"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/button15"
            android:layout_alignBottom="@+id/button15"
            android:layout_marginRight="40dp"
            android:layout_toLeftOf="@+id/button14"
            android:paddingBottom="15dp"
            android:paddingLeft="55dp"
            android:paddingRight="55dp"
            android:paddingTop="15dp"
            android:text="@string/Transmit"
            android:textSize="15dp"
            android:typeface="sans" 
            android:onClick="onCLickTransmit" />

        <EditText
            android:id="@+id/editText1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/button13"
            android:layout_alignBottom="@+id/button13"
            android:layout_alignParentLeft="true"
            android:layout_alignRight="@+id/button16"
            android:ems="10"
            android:focusable="false"
            android:inputType="text"
            android:padding="15dp"
            android:textSize="20dp" >

            <requestFocus />
        </EditText>

        <EditText
            android:id="@+id/editText2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/button14"
            android:layout_alignBottom="@+id/button14"
            android:layout_alignParentLeft="true"
            android:layout_alignRight="@+id/editText1"
            android:ems="10"
            android:focusable="true"
            android:inputType="text"
            android:padding="15dp"
            android:textSize="20dp" />

        <Button
            android:id="@+id/button09"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/button16"
            android:layout_alignBottom="@+id/button16"
            android:layout_marginRight="44dp"
            android:layout_toLeftOf="@+id/button16"
            android:paddingBottom="15dp"
            android:paddingLeft="31dp"
            android:paddingRight="31dp"
            android:paddingTop="15dp"
            android:text="@string/SelectReader"
            android:textSize="15dp" 
            android:onClick="onCLickSelectReader"/>

        <Button
            android:id="@+id/button07"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/button08"
            android:layout_alignLeft="@+id/button15"
            android:layout_alignRight="@+id/button15"
            android:layout_alignTop="@+id/button08"
            android:text="@string/GetStatus"
            android:textSize="15dp" 
            android:onClick="onCLickTracking" />

        <Spinner
            android:id="@+id/spinner1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/button07"
            android:layout_alignParentLeft="true"
            android:layout_alignRight="@+id/button16"
            android:layout_alignTop="@+id/button07" />

        <Button
            android:id="@+id/Button06"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/button15"
            android:layout_alignRight="@+id/button07"
            android:layout_below="@+id/button08"
            android:layout_marginBottom="10dp"
            android:layout_marginTop="10dp"
            android:onClick="onClickGetAttrib"
            android:padding="15dp"
            android:text="@string/GetAttrib"
            android:textSize="15dp" 
            />

        <Button
            android:id="@+id/Button05"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/Button06"
            android:layout_alignBottom="@+id/Button06"
            android:layout_alignLeft="@+id/button08"
            android:layout_alignRight="@+id/button08"
            android:layout_marginBottom="10dp"
            android:layout_marginTop="10dp"
            android:padding="15dp"
            android:text="@string/SetAttrib"
            android:textSize="15dp" 
            android:onClick="onClickSetAttrib" android:visibility="invisible"/>

        <EditText
            android:id="@+id/EditText03"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/Button06"
            android:layout_alignBottom="@+id/Button06"
            android:layout_alignParentLeft="true"
            android:layout_alignRight="@+id/spinner1"
            android:ems="10"
            android:focusable="true"
            android:inputType="text"
            android:padding="15dp"
            android:textSize="10pt" />

        <EditText
            android:id="@+id/editText4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_alignRight="@+id/Button05"
            android:layout_below="@+id/Button06"
            android:layout_marginBottom="10dp"
            android:layout_marginTop="30dp"
            android:ems="10"
            android:inputType="textMultiLine" />

    </RelativeLayout>
